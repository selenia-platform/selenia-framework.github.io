<!DOCTYPE html>
<html>
  <head>
    <title>{% block title %}{{ page.title }}{% endblock %} &mdash; {{ site.title }} &mdash; {{ site.subtitle }}</title>
    <meta charset="utf-8">
    <meta name="theme-color" content="#ffffff">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% block head_meta %}
    <meta name="description" content="A modern php framework for faster and easier web development">
    <!-- Open Graph data -->
  <head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# article: http://ogp.me/ns/article#">
    <meta property="og:title" content="Selenia Framework"/>
    <meta property="og:type" content="website"/>
    <meta property="og:url" content="https://selenia.com/"/>
    <meta property="og:image" content="{{ site.url }}/images/selenia_100.png"/>
    <meta property="og:description" content="A modern php framework for faster and easier web development"/>
    <meta property="og:site_name" content="Selenia Framework"/>

    <meta name="robots" content="index, follow">
    {% endblock %}
    <link href="{{ site.url }}/components/bootstrap-3.3.4-dist/css/bootstrap.min.css" rel="stylesheet" type="text/css"/>
    <link href="{{ site.url }}/components/bootstrap-3.3.4-dist/css/bootstrap-theme.min.css"
          rel="stylesheet"
          type="text/css"/>
    <link href='https://fonts.googleapis.com/css?family=Orbitron:400,500,700,900' rel='stylesheet' type='text/css'>
    <link href="{{ site.url }}/css/style.css" rel="stylesheet" type="text/css"/>
    <link href="{{ site.url }}/css/animate.min.css" rel="stylesheet" type="text/css"/>
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements --><!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script><![endif]-->

    <link rel="apple-touch-startup-image" href="{{ site.url }}/images/selenia/2048x2048.png">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <link rel="shortcut icon" sizes="76x76" href="{{ site.url }}/images/selenia/76x76.png">
    <link rel="shortcut icon" sizes="120x120" href="{{ site.url }}/images/selenia/120x120.png">
    <link rel="shortcut icon" sizes="128x128" href="{{ site.url }}/images/selenia/128x128.png">
    <link rel="shortcut icon" sizes="152x152" href="{{ site.url }}/images/selenia/152x152.png">
    <link rel="shortcut icon" sizes="196x196" href="{{ site.url }}/images/selenia/196x196.png">
    <link rel="shortcut icon" sizes="512x512" href="{{ site.url }}/images/selenia/512x512.png">
    <link rel="shortcut icon" sizes="1024x1024" href="{{ site.url }}/images/selenia/1024x1024.png">
    <link rel="shortcut icon" sizes="2048x2048" href="{{ site.url }}/images/selenia/2048x2048.png">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ site.url }}/images/selenia/76x76.png">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ site.url }}/images/selenia/120x120.png">
    <link rel="apple-touch-icon" sizes="128x128" href="{{ site.url }}/images/selenia/128x128.png">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ site.url }}/images/selenia/152x152.png">
    <link rel="apple-touch-icon" sizes="196x196" href="{{ site.url }}/images/selenia/196x196.png">
    <link rel="apple-touch-icon" sizes="512x512" href="{{ site.url }}/images/selenia/512x512.png">
    <link rel="apple-touch-icon" sizes="1024x1024" href="{{ site.url }}/images/selenia/1024x1024.png">
    <link rel="apple-touch-icon" sizes="2048x2048" href="{{ site.url }}/images/selenia/2048x2048.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ site.url }}/images/selenia/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ site.url }}/images/selenia/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ site.url }}/images/selenia/favicon-16x16.png">

    <link rel="stylesheet" href="{{ site.url }}/components/highlightjs/styles/monokai.css"/>
    <link rel="alternate"
          type="application/atom+xml"
          href="{{ site.url }}/atom.xml"
          title="{{ site.title }} activity feed"/>

    {% block head_styles %} {% endblock %} {% block head_scripts %} {% endblock %}
  </head>
  <body>

    {% block master %}{% endblock %}

      {% include 'forkme' %}

    <script src="{{ site.url }}/components/jquery/jquery.min.js"></script>
    <script src="{{ site.url }}/components/bootstrap-3.3.4-dist/js/bootstrap.min.js"></script>
    <script src="{{ site.url }}/components/highlightjs/highlight.pack.js"></script>
    <script>hljs.initHighlightingOnLoad ();</script>

    {% block scripts %} {% endblock %}

    {% if site.google_analytics_tracking_id %}

      <script type="text/javascript">
        (function (i, s, o, g, r, a, m) {
          i['GoogleAnalyticsObject'] = r;
          i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push (arguments)
              }, i[r].l = 1 * new Date ();
          a = s.createElement (o),
              m = s.getElementsByTagName (o)[0];
          a.async = 1;
          a.src   = g;
          m.parentNode.insertBefore (a, m)
        }) (window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga ('create', '{{site.google_analytics_tracking_id}}', 'auto');
        ga ('send', 'pageview');
      </script>

    {% endif %}

    {% block scripts_after %} {% endblock %}

  </body>
</html>
